<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
    <constant name="struts.enable.DynamicMethodInvocation" value="false"/>

    <package name="myPackage" extends="struts-default">
        <action name="index">
        	<result>/index.jsp</result>
        </action>
        <action name="ir_registro" class="com.dred.struts.formularios.usuario.RellenaProvinciasAction">
            <result type="redirectAction">listar_usuarios</result>
        </action>
        <action name="listar_usuarios" method="mostrar" class="com.dred.struts.formularios.usuario.UsuarioAction">
            <result>/WEB-INF/jsp/registro.jsp</result>
            <result name="input">/WEB-INF/jsp/registro.jsp</result>
        </action>
        <action name="crear_usuario" class="com.dred.struts.formularios.usuario.UsuarioAction">
            <result type="redirectAction">listar_usuarios</result>
            <result name="input">/WEB-INF/jsp/registro.jsp</result>
        </action>
        
		 <action name="ir_nuevoProyecto">
            <result>/WEB-INF/jsp/nuevo_proyecto.jsp</result>
        </action>
        <action name="crear_proyecto" class="com.dred.struts.formularios.proyectos.ProyectoAccion">
            <result>/WEB-INF/jsp/proyecto_creado.jsp</result>
            <result name="input">/WEB-INF/jsp/nuevo_proyecto.jsp</result>
        </action>
    </package>

</struts>